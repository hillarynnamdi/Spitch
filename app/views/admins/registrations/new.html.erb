    <% provide(:title, 'Admin Register | Gofixam')%>

  <div class="login_wrapper">


<div class="logo_icon">
<%=link_to  root_path do  %>
  <%=image_tag 'logo.PNG',class:'img-thumbnail'%><% end -%>
</div>

<%= simple_form_for(resource, as: resource_name, url: registration_path(resource_name),:html=>{class:"form-signin"}) do |f| %>


  <%= f.error_notification %>
<div class="form-inputs">


<div class="form-group">
    <%= f.input :email, required: true,class:"form-control",placeholder:"Email Address"%>
 </div>

   
 <div class="form-group">
    <%= f.input :password, required: true, hint: ("#{@minimum_password_length} characters minimum" if @minimum_password_length),class:"form-control",placeholder:"Password" %>
 </div>

 <div class="form-group">
    <%= f.input :password_confirmation, required: true,class:"form-control",placeholder:"Password Confirmation" %>
 </div>
 
  <div class="form-actions">
    <%= f.button :submit, "Sign up",class:"btn btn-lg btn-primary btn-block" %>
  </div>

</div>

<% end %>


<div class="devise_login_links">
  <%= render "admins/shared/links" %>
  </div>
</div>


<%= render 'shared/footer2'%>




